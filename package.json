{
  "name": "telegram-scraper",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -e .env -- node --loader ts-node/esm src/main.ts",
    "price": "dotenv -e .env -- node --loader ts-node/esm src/price.ts",
    "price2": "dotenv -e .env -- node --loader ts-node/esm src/price2.ts",
    "-- HELPERS --": "-------------------------------------------------------------------------------------------------------",
    "prepare": "husky install",
    "ci": "yarn clean && yarn install",
    "clean": "yarn clean:packages",
    "clean:packages": "rm -fr yarn.lock package-lock.json dist node_modules",
    "format": "pretty-quick \"*.ts\" --write --staged",
    "lint": "eslint -c .eslintrc.js --ext .ts,.js .",
    "lint:fix": "yarn eslint --fix"
  },
  "dependencies": {
    "@exodus/prices": "0.0.2",
    "adm-zip": "0.5.9",
    "axios": "0.22.0",
    "cryptocompare": "1.0.0",
    "csv-parse": "4.16.3",
    "input": "1.0.1",
    "mkdirp": "1.0.4",
    "node-fetch": "3.0.0",
    "telegram": "1.9.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.15.8",
    "@commitlint/cli": "13.2.0",
    "@commitlint/config-conventional": "13.2.0",
    "@types/adm-zip": "0.4.34",
    "@types/mkdirp": "1.0.2",
    "@types/node": "16.10.3",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "dotenv-cli": "4.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.10.0",
    "husky": "7.0.2",
    "lint-staged": "11.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.1",
    "rimraf": "3.0.2",
    "ts-node": "10.2.1",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">= 14.x.x"
  },
  "lint-staged": {
    "**/*.ts": [
      "yarn format"
    ]
  }
}
